#include <stdio.h>
int main()
{
    int input ;
    printf("Enter a number for input: ") ;
    scanf("%d",&input) ;
    switch(input)
     {

        case 1 :
            printf("The input number is 1 \n") ;
            break ;

        case 2 :
            printf("The input number is 2 \n") ;
            break ;

        case 3 :
            printf("The input number is 3 \n") ;
            break ;

        default:
            printf("The input number is unknown\n") ;
            break ;

     }


    return 0 ;
}
